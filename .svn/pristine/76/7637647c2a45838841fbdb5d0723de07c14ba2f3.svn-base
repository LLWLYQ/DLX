    "node-notifier": "^5.2.1",
    "nodemon": "^1.14.12",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-loader": "2.0.9",
    "progress-bar-webpack-plugin": "^1.11.0",
    "qiniu": "^7.1.5",
    "rimraf": "^2.6.2",
    "style-loader": "0.19.1",
    "stylus": "0.54.5",
    "stylus-loader": "3.0.1",
    "url-loader": "0.6.2",
